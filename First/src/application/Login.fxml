<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <AnchorPane prefHeight="341.0" prefWidth="604.0">
         <children>
            <TextField fx:id="ID" layoutX="222.0" layoutY="142.0" promptText="ID" />
            <Button fx:id="btnLogin" layoutX="271.0" layoutY="213.0" mnemonicParsing="false" onAction="#Login" prefHeight="22.0" prefWidth="63.0" text="login" />
            <PasswordField fx:id="password" layoutX="222.0" layoutY="166.0" promptText="password" />
            <CheckBox layoutX="530.0" layoutY="304.0" mnemonicParsing="false" text="admin" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="203.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/logo_footer.gif" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
